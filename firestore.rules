rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Estas reglas permiten el acceso de lectura y escritura a toda tu base de datos.
    // Son solo para fines de diagn√≥stico y deben ser reemplazadas.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
